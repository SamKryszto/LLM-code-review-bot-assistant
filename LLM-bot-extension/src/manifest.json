{
    "name": "GitHub_LLM_Bot",
    "version": "1.0.0",
    "description": "Bot assistant pour la revue de code avec ChatGPT",
    "manifest_version": 3,
    "action":{
        "default_popup": "index.html",
        "default_title": "GitHub_LLM_Bot"
